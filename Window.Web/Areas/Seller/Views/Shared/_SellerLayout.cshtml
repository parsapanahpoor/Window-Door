Version: 1.6.0
Purchase: https://wrapbootstrap.com/theme/beyondadmin-adminapp-angularjs-mvc-WB06R48S4
-->
<html xmlns="http://www.w3.org/1999/xhtml">
<!-- Head -->
<head>
    <meta charset="utf-8" />
    <title>@ViewData["Title"]</title>
    <meta name="description" content="Dashboard" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <partial name="_SellerHeaderReferences" />
    @RenderSection("Styles", false)
</head>
<body>
    @await Component.InvokeAsync("SellerNavbar")
    <div class="main-container container-fluid">
        <div class="page-container">
            <div class="page-content" style="margin-right: 0px;">
                <div class="page-breadcrumbs">
                    <ul class="breadcrumb">
                        <li>
                            <i class="fa fa-home"></i>
                            <a asp-area="Seller" asp-controller="Home" asp-action="Index">@SharedLocalizer["Home"]</a>
                        </li>
                        @RenderSection("breadcrumb", true)
                    </ul>
                </div>
                <partial name="_SellerPageHeader" />
                <div class="page-body">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>
    <partial name="_SellerFooterReferences" />
    <partial name="_Notification" />
    <partial name="_SweetAlertLocalizer" />
    <partial name="_Modals" />
    @RenderSection("Scripts", false)
</body>
</html>
